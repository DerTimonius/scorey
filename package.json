{
  "name": "scorey",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "exec biome lint . --write",
    "lint:unsafe": "exec biome lint . --write --unsafe",
    "lint:assist": "exec biome check --formatter-enabled=false --linter-enabled=false --write",
    "lint:all": "pnpm lint && pnpm lint:unsafe && pnpm lint:assist",
    "format": "exec biome format . --write",
    "check": "exec biome check .",
    "check:write": "exec biome check . --write",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit --project tsconfig.app.json",
    "test:e2e": "exec playwright test",
    "test:e2e:debug": "exec playwright test --debug",
    "test:e2e:ui": "exec playwright test --ui",
    "knip": "knip"
  },
  "dependencies": {
    "@fontsource-variable/victor-mono": "^5.2.8",
    "@fontsource/rampart-one": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@number-flow/react": "^0.5.10",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jotai": "^2.16.1",
    "lucide-react": "^0.525.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "react-i18next": "^15.7.4",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "knip": "^5.78.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-pwa": "^1.2.0"
  },
  "packageManager": "pnpm@10.26.2"
}
