{
  "name": "scorey",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "exec biome lint . --write",
    "lint:unsafe": "exec biome lint . --write --unsafe",
    "lint:assist": "exec biome check --formatter-enabled=false --linter-enabled=false --write",
    "lint:all": "pnpm lint && pnpm lint:unsafe && pnpm lint:assist",
    "format": "exec biome format . --write",
    "check": "exec biome check .",
    "check:write": "exec biome check . --write",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit --project tsconfig.app.json",
    "test:e2e": "exec playwright test",
    "test:e2e:debug": "exec playwright test --debug",
    "test:e2e:ui": "exec playwright test --ui",
    "knip": "knip"
  },
  "dependencies": {
    "@fontsource-variable/victor-mono": "^5.2.6",
    "@fontsource/rampart-one": "^5.2.6",
    "@hookform/resolvers": "^5.2.0",
    "@number-flow/react": "^0.5.10",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@tailwindcss/vite": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jotai": "^2.12.5",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-i18next": "^15.6.0",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@playwright/test": "^1.54.1",
    "@types/node": "^24.0.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "knip": "^5.62.0",
    "tw-animate-css": "^1.3.5",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.0.1"
  },
  "packageManager": "pnpm@10.13.1"
}
